{% extends 'base.html.twig' %}

{% block bodyAtt %}class="fixed-left"{% endblock %}

{% block body %}
<div id="wrapper">
    {{ include('Bloques/menu.html.twig') }}
    {{ include('Bloques/menuIzquierdo.html.twig') }}
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container">
                    {% block content %}
                        <div class="row">
                            <div class="col-xs-12">
                                <br/>
                                {% if app.session.get('soporteInactivo') %}
                                    <div class="alert alert-danger alert-dismissible" role="alert">
                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                        <strong>Soporte inactivo!</strong> La mesa de ayuda se encuentra inactiva por incumplimiento en el pago
                                    </div>
                                {% endif %}
                                <div class="contentimagecentral">
                                <img class="centerimage" src="{{ asset('publicImages/trabajo.png') }}"/>
                                </div>
                            </div>
                        </div>
            {{ include('Bloques/pie.html.twig') }}
                    {% endblock %}
{% endblock %}